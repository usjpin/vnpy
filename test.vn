config width 800;
config height 500;
config mode "graphic";
config volume 0.7;

audio start "cmon.mp3";
image show "img/red.png";
display "sup";
options {
    case "pick me 1" do display "1 picked";
    case "pick me 2" do display "2 picked";
    case "pick me 3" do display "3 picked";
    case "pick me 3" do display "4 picked";
}
display "over";
delay 20;
//image show "img/blue.png";
//delay 2;
scene a {
    image show "img/red.png";
    //audio start "cmon.mp3";
    display "sup";
    delay 3;
    display "yeet";
    //audio stop "cmon.mp3";
    delay 2;
    exit;
}
//delay 1;
scene b {
    image show "img/blue.png";
    display "bro";
    options {
        case "pick me" do delay 2;
    }
    delay 30;
    jump a;
}
jump b;
